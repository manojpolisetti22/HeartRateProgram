                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <GridPane nodeOrientation="LEFT_TO_RIGHT" prefHeight="141.0" prefWidth="600.0">
